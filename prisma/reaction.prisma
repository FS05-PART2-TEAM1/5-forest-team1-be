model Reaction {
  id        String   @id @default(uuid())
  studyId   String
  emoji     String
  counts    Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  ///study Study @relation(fields: [studyId], references: [id], onDelete: Cascade)

  @@index([studyId])
}
